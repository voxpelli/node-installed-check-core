{
  "name": "installed-check-core",
  "version": "3.0.0-0",
  "description": "Checks that all dependencies in your package.json have supported versions installed",
  "homepage": "http://github.com/voxpelli/node-installed-check-core",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-installed-check-core.git"
  },
  "main": "index.js",
  "scripts": {
    "check:dependency-check": "dependency-check *.js 'lib/**/*.js' --no-dev",
    "check:installed-check": "installed-check -e -i eslint",
    "check:eslint": "eslint .",
    "test": "run-p check:*"
  },
  "keywords": [],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@voxpelli/semver-set": "^1.0.1",
    "read-installed": "^4.0.3",
    "read-package-json": "^2.0.3",
    "semver": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^12.7.1",
    "dependency-check": "^4.1.0",
    "eslint": "^6.1.0",
    "eslint-config-semistandard": "^14.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "installed-check": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.5.3"
  }
}
