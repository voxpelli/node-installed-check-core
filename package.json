{
  "name": "installed-check-core",
  "version": "5.0.0-0",
  "description": "Checks that all dependencies in your package.json have supported versions installed",
  "homepage": "http://github.com/voxpelli/node-installed-check-core",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-installed-check-core.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "index.d.ts.map",
    "lib/*.js",
    "lib/*.d.ts",
    "lib/*.d.ts.map"
  ],
  "scripts": {
    "build:0": "run-s clean",
    "build:1-declaration": "tsc -p declaration.tsconfig.json",
    "build:2-add-ignores": "ts-ignore-import '**/*.d.ts'",
    "build": "run-s build:*",
    "check:0": "run-s clean",
    "check:1:dependency-check": "dependency-check *.js 'test/**/*.js' --no-dev",
    "check:1:installed-check": "installed-check",
    "check:1:lint": "eslint .",
    "check:1:tsc": "tsc",
    "check:1": "run-p -c --aggregate-output check:1:*",
    "check:2": "run-s build:1-declaration",
    "check:3": "type-coverage --detail --strict --at-least 100 --ignore-files 'test/*'",
    "check": "run-s check:*",
    "clean:declarations": "rm -rf $(find . -maxdepth 2 -type f -name '*.d.ts')",
    "clean": "run-p clean:*",
    "prepublishOnly": "run-s build",
    "sync-gh-actions": "ghat",
    "test:mocha": "c8 --reporter=lcov --reporter text mocha 'test/**/*.spec.js'",
    "test-ci": "run-s test:*",
    "test": "run-s clean check test:*"
  },
  "keywords": [],
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@voxpelli/semver-set": "^3.0.0",
    "list-installed": "^1.0.1",
    "read-pkg": "^5.2.0",
    "semver": "^7.3.4",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "@hdsydsvenskan/ts-ignore-import": "^2.0.0",
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "^12.20.15",
    "@types/semver": "^7.3.6",
    "@types/verror": "^1.10.4",
    "@voxpelli/eslint-config": "11.0.1",
    "@voxpelli/eslint-config-jsdoc-ts": "^0.3.1",
    "@voxpelli/tsconfig": "^2.0.1",
    "c8": "^7.7.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "dependency-check": "^4.1.0",
    "eslint": "^7.28.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^30.7.13",
    "eslint-plugin-mocha": "^8.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "ghat": "^0.14.0",
    "husky": "^4.3.8",
    "installed-check": "^4.0.0",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "type-coverage": "^2.17.5",
    "type-fest": "^0.21.3",
    "typescript": "^4.3.2"
  }
}
